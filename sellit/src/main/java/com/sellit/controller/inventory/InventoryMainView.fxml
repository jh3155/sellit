            <Button mnemonicParsing="false" onAction="#showProductList" prefHeight="50.0" prefWidth="220.0" text="Product">
               <styleClass>
                  <String fx:value="info" />
                  <String fx:value="flat-button" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#showDepartmentList" prefHeight="50.0" prefWidth="220.0" text="Department">
               <styleClass>
                  <String fx:value="info" />
                  <String fx:value="flat-button" />
               </styleClass>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
